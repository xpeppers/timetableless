{
  "name": "timetableless",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "deploy": "sh -c 'npm run deploy:server $0 && npm run deploy:client $0'",
    "deploy:client": "sh -c 'cd client && npm run build $0 && cd - && sls client deploy --profile xpeppers-develop --no-confirm --stage $0'",
    "deploy:server": "sh -c 'sls deploy --profile xpeppers-develop --stage $0'",
    "install:dynamodb": "serverless dynamodb install",
    "logs": "serverless logs",
    "remove": "sls remove --profile xpeppers-develop && sls client remove --profile xpeppers-develop",
    "sls": "serverless",
    "start": "serverless offline start",
    "test": "npm run test:unit && npm run test:integration",
    "test:acceptance": "npm start -- --exec 'mocha test/acceptance/**'",
    "test:integration": "npm start -- --exec 'mocha test/integration/**'",
    "test:unit": "mocha test/** --exclude test/acceptance/** --exclude test/integration/**"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dallagi/timetableless.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dallagi/timetableless/issues"
  },
  "homepage": "https://github.com/dallagi/timetableless#readme",
  "devDependencies": {
    "mocha": "^7.0.1",
    "serverless": "^1.63.0",
    "serverless-dynamodb-local": "^0.2.39",
    "serverless-finch": "^2.5.2",
    "serverless-offline": "^5.12.1",
    "sinon": "^9.0.0"
  },
  "dependencies": {
    "axios": "^0.19.2"
  }
}
